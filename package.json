{
  "name": "css",
  "version": "0.0.0-development",
  "repository": {
    "type": "git",
    "url": "https://github.com/komplib/css.git"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,json,md,yml,yaml,css,scss,less,vue}\" --ignore-unknown",
    "install:ci": "yarn install --immutable --immutable-cache --check-cache",
    "lint": "yarn eslint --ext **/*.{js,ts} src",
    "postinstall": "husky install",
    "postpack": "pinst --enable",
    "postpublish": "pinst --enable",
    "prepack": "pinst --disable",
    "prepublishOnly": "pinst --disable",
    "semantic-release": "semantic-release",
    "test": "echo \"Error: no test specified\""
  },
  "packageManager": "yarn@3.2.1",
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "latest",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "pinst": "^3",
    "prettier": "2.6.2",
    "semantic-release": "^19.0.2"
  },
  "lint-staged": {
    "*.{js,json,md,yml,yaml,css,scss,less,vue}": "yarn format"
  }
}
